# Backend Configuration
BACKEND_PORT=5055
SECRET_KEY=dev-secret-key-change-in-production
ENVIRONMENT=docker
DOCKER_ENV=true

# Database Configuration
POSTGRES_DB=mental_health
POSTGRES_USER=ai_buddy
POSTGRES_PASSWORD=ai_buddy_password
POSTGRES_PORT=5432
# SQLAlchemy-compatible connection string (local Postgres)
DATABASE_URL=postgresql+psycopg://ai_buddy:ai_buddy_password@localhost:5432/mental_health

# Redis Configuration
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# AI Provider Configuration
GEMINI_API_KEY=your_gemini_api_key_here
# Optional: comma-separated Gemini API keys for rotation
GEMINI_API_KEYS=key1,key2,key3
OPENAI_API_KEY=your_openai_api_key_here
PPLX_API_KEY=your_perplexity_api_key_here
AI_PROVIDER=gemini

# Logging
AI_DEBUG_LOGS=false
LOG_LEVEL=INFO

# Community Feature Flags
COMMUNITY_ENABLED=true
COMMUNITY_POSTING_ENABLED=false
TEMPLATES_ONLY=true
RATE_LIMITS_COMMUNITY_FEED="120 per minute"
RATE_LIMITS_REACTION="20 per minute; 200 per day"
RATE_LIMITS_REPORT="10 per minute; 100 per day"

# Flutter Web Configuration
FLUTTER_WEB_PORT=8080

# Version and Build Info
VERSION=1.0.0
BUILD_TIME=unknown